  </select>

  <!--// 2. 查找7天每天聊天数-->
  <select id="selectDailyChatCount" resultType="map">
    SELECT DATE_FORMAT(time, '%Y-%m-%d') as chat_date, COUNT(*) as chat_count
    FROM user_chat
