  <!--// 1. 查找7天每天登录人数-->
  <select id="selectDailyLoginCount" resultType="map">
    SELECT DATE_FORMAT(last_login_time, '%Y-%m-%d') as login_date, COUNT(DISTINCT user_id) as login_count
    FROM user
    WHERE last_login_time BETWEEN #{startDate} AND #{endDate}
